{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "moduleResolution": "Node",
        "rootDir": "./",
        "outDir": "./dist",
        "strict": false,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true
    },
    "include": ["./"],
    "exclude": ["node_modules", "dist"]
}
